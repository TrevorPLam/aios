    "expo:clean": "node -e \"const fs=require('fs');['node_modules/.cache','.expo','.metro-cache'].forEach(d=>{try{fs.rmSync(d,{recursive:true,force:true})}catch(e){}})\" && npx expo start --clear",
    "expo:clean:full": "node -e \"const fs=require('fs');['.expo','.metro-cache','node_modules','package-lock.json'].forEach(d=>{try{fs.rmSync(d,{recursive:true,force:true})}catch(e){}})\" && npm install",
    "expo:clean:native": "node -e \"const fs=require('fs');['node_modules/.cache','.expo','.metro-cache'].forEach(d=>{try{fs.rmSync(d,{recursive:true,force:true})}catch(e){}})\" && (npx watchman watch-del-all 2>$null || echo 'watchman not available') && npm install",
