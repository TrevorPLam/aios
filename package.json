{
  "name": "my-app",
  "main": "client/index.js",
  "version": "1.0.0",
  "scripts": {
    "start": "npx expo start",
    "expo:dev": "EXPO_PACKAGER_PROXY_URL=https://$REPLIT_DEV_DOMAIN REACT_NATIVE_PACKAGER_HOSTNAME=$REPLIT_DEV_DOMAIN EXPO_PUBLIC_DOMAIN=$REPLIT_DEV_DOMAIN:5000 npx expo start --localhost --ios",
    "expo:clean": "rm -rf node_modules/.cache .expo .metro-cache && npx expo start --clear",
    "expo:clean:full": "rm -rf .expo .metro-cache node_modules package-lock.json && npm install",
    "expo:clean:native": "rm -rf node_modules/.cache .expo .metro-cache && watchman watch-del-all 2>/dev/null || true && npm install",
    "expo:rebuild:ios": "npx expo prebuild --clean && npx expo run:ios",
    "expo:rebuild:android": "npx expo prebuild --clean && npx expo run:android",
    "server:dev": "NODE_ENV=development tsx server/index.ts",
    "expo:start:static:build": "npx expo start --no-dev --minify --localhost",
    "expo:static:build": "node scripts/build.js",
    "server:build": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=server_dist",
    "server:prod": "NODE_ENV=production node server_dist/index.js",
    "db:push": "drizzle-kit push",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "npx expo lint",
    "lint:fix": "npx expo lint --fix",
    "check:types": "tsc --noEmit",
    "check:format": "prettier --check \"**/*.{js,ts,tsx,css,json}\"",
    "format": "prettier --write \"**/*.{js,ts,tsx,css,json}\"",
    "compile:constitution": "node scripts/tools/compile-constitution.mjs",
    "check:exceptions": "node scripts/tools/check-exceptions.mjs",
    "check:traceability": "node scripts/tools/check-traceability.mjs",
    "check:agent-platform": "node scripts/tools/check-agent-platform.mjs",
    "check:worklets": "node scripts/check-worklets-version.mjs",
    "check:expo-config": "node scripts/check-expo-config.mjs",
    "check:postinstall": "node scripts/post-install-check.mjs",
    "check:deps": "node scripts/deep-dependency-check.mjs",
    "check:startup": "node scripts/check-startup-blockers.mjs",
    "postinstall": "node scripts/post-install-check.mjs"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/slider": "5.0.1",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.9.5",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/native-stack": "^7.10.0",
    "@tanstack/react-query": "^5.90.19",
    "@types/pako": "^2.0.4",
    "bcryptjs": "^3.0.3",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "expo": "^54.0.31",
    "expo-av": "~16.0.7",
    "expo-blur": "^15.0.8",
    "expo-constants": "~18.0.9",
    "expo-contacts": "^15.0.11",
    "expo-file-system": "^19.0.21",
    "expo-font": "~14.0.9",
    "expo-glass-effect": "~0.1.8",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-image-manipulator": "^14.0.8",
    "expo-image-picker": "^17.0.10",
    "expo-linking": "~8.0.11",
    "expo-media-library": "^18.2.1",
    "expo-speech": "~14.0.7",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "express": "^4.21.2",
    "http-proxy-middleware": "^3.0.5",
    "jsonwebtoken": "^9.0.3",
    "pako": "^2.1.0",
    "pg": "^8.17.1",
    "react": "19.1.0",
    "react-dom": "19.2.3",
    "react-native": "0.81.5",
    "react-native-draggable-flatlist": "^4.0.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-controller": "1.18.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "tsx": "^4.20.6",
    "winston": "^3.19.0",
    "ws": "^8.19.0",
    "zod": "^3.24.2",
    "zod-validation-error": "^5.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/bcryptjs": "^3.0.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.9",
    "@types/react": "~19.1.10",
    "@types/winston": "^2.4.4",
    "babel-jest": "^30.2.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.39.2",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "~29.7.0",
    "prettier": "3.8.0",
    "react-test-renderer": "19.1.0",
    "ts-jest": "^29.4.6",
    "typescript": "~5.9.2",
    "yaml": "^2.8.2"
  },
  "private": true
}
